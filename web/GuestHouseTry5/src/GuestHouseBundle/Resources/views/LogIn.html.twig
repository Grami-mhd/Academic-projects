                    {% block formlogin %}
                    {% endblock %}

                </div>

                <div class="cont_form_sign_up">
                    <a href="" onclick="ocultar_login_sign_up()"><i class="material-icons">&#xE5C4;</i></a>
                    <h2>SIGN UP</h2>


                        <div>
                            <br>
                            <a href="#" title="login with facebook" target="_blank"> <img src="{{asset ('bundles/guesthouse/pics/facebook-logo.png')}}" alt=""></a><br>
                            <a href="#" title="login with facebook" target="_blank"> <img src="{{asset ('bundles/guesthouse/pics/google-logo.png')}}" alt=""></a><br>
                            <a href="#" title="login with facebook" target="_blank"> <img src="{{asset ('bundles/guesthouse/pics/twitter-logo.png')}}" alt=""></a>
                            <br>
                            <a href="register" target="_blank"> <input type="button" value="Sign Up Here" class="btn_sign_up" id="55" style="text-align: center"></a>

                        </div>

                </div>

            </div>

        </div>
    </div>
</div>

<script src="{{asset ('bundles/guesthouse/js/index.js')}}"></script>

